<h1> <%= @team.name %> </h1>
<h3> <%=     @team.city %> </h3>

<h2>Players</h2>
<% @team.players.each do |player| %>
         <%= link_to player.name, player_path(player) %><br><br>
         <% end %>

<%= link_to "Edit this team", edit_team_path(@team) %>
<br><br>
<%= link_to "Delete this team", @team, method: :delete %>
<br><br>
<%= link_to "Back to all teams", teams_path %>
